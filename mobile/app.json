{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "scheme": "mobile",
    "newArchEnabled": true, 
    "ios": {
      "bundleIdentifier": "com.mosesdata.sokoni" 
    },
    "android": {
      "package": "com.mosesdata.sokoni"
    },
    "plugins": [
      "expo-router",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.sokoni",
          "androidEnableGooglePay": true
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "project": "react-native-mobile",
          "organization": "moseomwa"
        }
      ]
    ]
  }
}